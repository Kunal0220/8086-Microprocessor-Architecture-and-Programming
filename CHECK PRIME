; CHECK IF A GIVEN NUMBER IS PRIME
; IF PRIME --> STORE FF IN CH   // CAN ALSO STORE A NUMBER IN MEMORY IF PRIME
;       ELES--> STORE 00 IN CH  // STORE 00 IN MEMORY IF NOT PRIME

; NUMBER>2 IN A REGISTER
MOV DX, 17
MOV CL, 0

MOV BL, 1

BACK:     
MOV AX, DX     
DIV BL
CMP AH, 0      
JNZ INC_INDEX
INC CL; INCREMENT FACTORS

INC_INDEX:
INC BL  

; CHECK IF INDEX IS LESS THAN THE GIVEN NUBEER
CMP DL, BL
JNE BACK

CMP CL, 1
JZ  NEXT
MOV CH, 0H
JMP HALT

NEXT:
MOV CH, 0FFH    ; PRIME

HALT:
HLT
