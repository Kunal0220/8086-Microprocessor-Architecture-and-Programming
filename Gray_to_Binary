; GRAY TO BINARY

MOV AL, 40H ; EXAMPLE ONLY, YOU CAN CHANGE THE INPUT HERE
MOV BL, AL

SHR BL,1
XOR AL, BL

SHR BL,1
XOR AL, BL

SHR BL,1
XOR AL, BL

SHR BL,1
XOR AL, BL                                    
          
SHR BL,1
XOR AL, BL

SHR BL,1
XOR AL, BL

SHR BL,1
XOR AL, BL ; RESULT IN AL REGISTER  7FH

